  "main": "serves.js",
  "type": "commonjs",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "env-cmd -f .env.prod node server",
    "dev": "env-cmd -f .env.dev nodemon server",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --quiet --fix",
    "lint:scripts:hook": "eslint . --ext .js",
    "prettier:cli": "prettier \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -l",
    "prettier:fix": "npm run prettier:cli --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.29.4",
    "mongoose": "^7.0.5",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "rate-limit-mongo": "^2.3.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olehyaremyn7/crm-app-backend.git"
