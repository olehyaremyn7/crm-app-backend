{
  "version": 2,
  "name": "crm-app-backend",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "uploads/**",
      "use": "@vercel/static"
  }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "server.js"
    },
    {
      "src": "/(.+)",
      "dest": "uploads/$1"
  }
  ]
}
